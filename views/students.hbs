<h1>Students List </h1>

<table border="1">
    <thead>
        <th>Stuent Id</th>
        <th>Name</th>
        <th>Program</th>
        <th>GPA</th>
    </thead>
    {{#each data}}
    <tr>
        <td>{{studId}}</td>
        <td>{{name}}</td>
        <td>{{program}}</td>
        <td>{{gpa}}</td>
    </tr>
    {{/each}}
</table>

